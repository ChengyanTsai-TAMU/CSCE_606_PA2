<tr id="<%= dom_id(movie) %>">
  <td><%= movie.title %></td>
  <td><%= movie.rating %></td>
  <td><%= movie.release_date.strftime("%Y-%m-%d") %></td>
  <td>
    <%= link_to "Show this movie", movie %> |  
    <%= link_to "Edit this movie", edit_movie_path(movie) %> 
    <%= button_to "Destroy this movie", movie, method: :delete, remote: true, data: { confirm: "Are you sure?" } %>
  </td>
</tr>


